
    <% @songs.each do |song| %>
      
        <%= song.title %></td>
       <%= song.release_year %></td>
        <%= song.genre %></td>
      <%= song.artist_name %></td>
        
          <%= link_to 'View', song %> |
          <%= link_to 'Edit', edit_song_path(song) %> |
          <%= link_to 'Delete', song, method: :delete, data: { confirm: 'For real?' } %>
        
     
    <% end %>
